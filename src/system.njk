    <hr>
    
    {% set activeMembers = [] %}
    {% set dormantMembers = [] %}
    
    {% for member in systemMembers %}
      {% if member.dormant %}
        {% set dormantMembers = (dormantMembers.push(member), dormantMembers) %}
      {% else %}
        {% set activeMembers = (activeMembers.push(member), activeMembers) %}
      {% endif %}
    {% endfor %}
    
    {% if activeMembers.length > 0 %}
    <p>
    active.
      {% for member in activeMembers %}
        <a href="{{ member.url }}">
          {{ member.displayName }}{% if not loop.last %}.{% endif %}
        </a>
      {% endfor %}
    </p>
    {% endif %}
    
    {% if dormantMembers.length > 0 %}
    <p>
    dormant.
      {% for member in dormantMembers %}
        <a href="{{ member.url }}">
          {{ member.displayName }}{% if not loop.last %}.{% endif %}
        </a>
      {% endfor %}
    </p>
    {% endif %}
  </div>
</main>
